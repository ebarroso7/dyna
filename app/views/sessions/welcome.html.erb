<% if logged_in? %>
  <h1>Welcome, <%= current_user.first_name %>! Do You Need Anything?</h1>
  <% if @saved_locations.present? %>
    <%= tag.div id: :saved_locations, data: { saved_locations: @saved_locations.to_json() } %>
  <% end %>

  <br>

<% else %>
  <%= image_tag "clean-Car.png" %>
  <br>
  <%= button_to "Login", '/login', method: :get %>
  <%= button_to "Sign Up", '/users/new', method: :get %>
<% end %>
